{
    "$schema": "https://schemas.wp.org/wp/6.7/theme.json",
    "version": 3,
    "settings": {
        "appearanceTools": false,
        "useRootPaddingAwareAlignments": false,
        "color": {
            "defaultDuotone": false,
            "defaultGradients": false,
            "defaultPalette": false,
            "palette": [
                {
                    "color": "#FFFFFF",
                    "name": "Base",
                    "slug": "base"
                },
                {
                    "color": "#000",
                    "name": "Contrast",
                    "slug": "contrast"
                },
                {
                    "color": "#FF6920",
                    "name": "Accent 1",
                    "slug": "accent-1"
                },
                {
                    "color": "#686868",
                    "name": "Accent 2",
                    "slug": "accent-2"
                },
                {
                    "color": "color-mix(in srgb, currentColor 20%, transparent)",
                    "name": "Accent 3",
                    "slug": "accent-3"
                }
            ]
        },
        "layout": {
            "contentSize": "1000px",
            "wideSize": "1200px"
        },
        "spacing": {
            "defaultSpacingSizes": false,
            "spacingSizes": [
                {
                    "name": "Tiny",
                    "size": "10px",
                    "slug": "20"
                },
                {
                    "name": "X-Small",
                    "size": "20px",
                    "slug": "30"
                },
                {
                    "name": "Small",
                    "size": "30px",
                    "slug": "40"
                },
                {
                    "name": "Regular",
                    "size": "clamp(30px, 5vw, 50px)",
                    "slug": "50"
                },
                {
                    "name": "Large",
                    "size": "clamp(30px, 7vw, 70px)",
                    "slug": "60"
                },
                {
                    "name": "X-Large",
                    "size": "clamp(50px, 7vw, 90px)",
                    "slug": "70"
                },
                {
                    "name": "XX-Large",
                    "size": "clamp(70px, 10vw, 140px)",
                    "slug": "80"
                }
            ],
            "units": [
                "%",
                "px",
                "em",
                "rem",
                "vh",
                "vw"
            ]
        },
        "typography": {
            "writingMode": true,
            "defaultFontSizes": false,
            "fluid": true,
            "fontSizes": [
                {
                    "fluid": false,
                    "name": "Small",
                    "size": "0.875rem",
                    "slug": "small"
                },
                {
                    "name": "Medium",
                    "size": "1rem",
                    "slug": "medium"
                },
                {
                    "fluid": {
                        "max": "1.375rem",
                        "min": "1.125rem"
                    },
                    "name": "Large",
                    "size": "1.38rem",
                    "slug": "large"
                },
                {
                    "fluid": {
                        "max": "2rem",
                        "min": "1.75rem"
                    },
                    "name": "Extra Large",
                    "size": "1.75rem",
                    "slug": "x-large"
                },
                {
                    "fluid": {
                        "max": "3rem",
                        "min": "2.15rem"
                    },
                    "name": "Extra Extra Large",
                    "size": "2.15rem",
                    "slug": "xx-large"
                }
            ],
            "fontFamilies": [
                {
                    "name": "SourceSansPro",
                    "slug": "source-sans-pro",
                    "fontFamily": "\"SourceSansPro\"",
                    "fontFace": [
                        {
                            "src": [
                                "file:./assets/fonts/transfonter.org-20250903-130001/SourceSansPro-Regular.woff2",
                                "file:./assets/fonts/transfonter.org-20250903-130001/SourceSansPro-Regular.woff"
                            ],
                            "fontWeight": "400 800",
                            "fontStyle": "normal",
                            "fontFamily": "\"SourceSansPro\""
                        }
                    ]
                },
                {
                    "name": "ZektonHv",
                    "slug": "zekton-hv",
                    "fontFamily": "\"ZektonHv\"",
                    "fontFace": [
                        {
                            "src": [
                                "file:./assets/fonts/transfonter.org-20250903-130001/ZektonHv-Regular.woff2",
                                "file:./assets/fonts/transfonter.org-20250903-130001/ZektonHv-Regular.woff"
                            ],
                            "fontWeight": "400 800",
                            "fontStyle": "normal",
                            "fontFamily": "\"ZektonHv\""
                        }
                    ]
                },
                {
                    "name": "GoldNight",
                    "slug": "gold-night",
                    "fontFamily": "\"GoldNight\"",
                    "fontFace": [
                        {
                            "src": [
                                "file:./assets/fonts/transfonter.org-20250903-130001/Gold Night.woff2",
                                "file:./assets/fonts/transfonter.org-20250903-130001/Gold Night.woff"
                            ],
                            "fontWeight": "400 800",
                            "fontStyle": "normal",
                            "fontFamily": "\"GoldNight\""
                        }
                    ]
                }
            ]
        },
        "blocks": {
            "core/site-logo": {
                "spacing": {
                    "blockGap": false
                },
                "dimensions": {
                    "aspectRatio": false
                }
            },
            "core/navigation": {
                "custom": {
                    "iconSize": "45px"
                }
            },
            "sws2025/image-mapper": {
                "typography": {
                    "fontFamily": false
                },
                "elements": {
                    "h2": {
                        "typography": {
                            "fontFamily": true,
                            "fontFamilies": [
                                {
                                    "name": "SourceSansPro",
                                    "slug": "source-sans-pro",
                                    "fontFamily": "\"SourceSansPro\""
                                },
                                {
                                    "name": "ZektonHv",
                                    "slug": "zekton-hv",
                                    "fontFamily": "\"ZektonHv\""
                                },
                                {
                                    "name": "GoldNight",
                                    "slug": "gold-night",
                                    "fontFamily": "\"GoldNight\""
                                }
                            ]
                        }
                    }
                }
            }
        }
    },
    "styles": {
        "color": {
            "background": "var:preset|color|base",
            "text": "var:preset|color|contrast"
        },
        "spacing": {
            "blockGap": "1.2rem"
        },
        "typography": {
            "fontFamily": "var:preset|font-family|source-sans-pro",
            "fontSize": "var:preset|font-size|medium",
            "fontWeight": "400",
            "lineHeight": "140%"
        },
        "elements": {
            "main": {
                "spacing": {
                    "margin": {
                        "left": "auto",
                        "right": "auto"
                    },
                    "padding": {
                        "left": "var:preset|spacing|50",
                        "right": "var:preset|spacing|50"
                    }
                }
            },
            "h1": {
                "color": {
                    "text": "var:preset|color|accent-1"
                },
                "typography": {
                    "fontFamily": "var:preset|font-family|zekton-hv",
                    "lineHeight": "140%"
                }
            },
            "h2": {
                "color": {
                    "text": "var:preset|color|accent-1"
                },
                "typography": {
                    "fontFamily": "var:preset|font-family|zekton-hv",
                    "lineHeight": "140%",
                    "fontSize": "var:preset|font-size|x-large"
                }
            },
            "h3": {
                "color": {
                    "text": "var:preset|color|accent-1"
                },
                "typography": {
                    "fontFamily": "var:preset|font-family|zekton-hv",
                    "lineHeight": "140%"
                }
            },
            "h4": {
                "color": {
                    "text": "var:preset|color|accent-1"
                },
                "typography": {
                    "fontFamily": "var:preset|font-family|zekton-hv",
                    "lineHeight": "140%"
                }
            },
            "h5": {
                "color": {
                    "text": "var:preset|color|accent-1"
                },
                "typography": {
                    "fontFamily": "var:preset|font-family|zekton-hv",
                    "lineHeight": "140%"
                }
            },
            "h6": {
                "color": {
                    "text": "var:preset|color|accent-1"
                },
                "typography": {
                    "fontFamily": "var:preset|font-family|zekton-hv",
                    "lineHeight": "140%"
                }
            }
        },
        "blocks": {
            "core/group": {
                "spacing": {
                    "margin": {
                        "left": "auto",
                        "right": "auto"
                    }
                }
            },
            "sws2025/image-mapper": {
                "border": {
                    "radius": "100px"
                },
                "spacing": {
                    "blockGap": "var:preset|spacing|50"
                }
            },
            "core/site-logo": {
                "spacing": {
                    "margin": "0",
                    "padding": "0"
                }
            }
        }
    },
    "templateParts": [
        {
            "area": "header",
            "name": "header",
            "title": "Header"
        },
        {
            "area": "footer",
            "name": "footer",
            "title": "Footer"
        }
    ]
}